		"@types/passport-jwt": "^4.0.1",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-express": "^4.1.6",
		"@zenstackhq/openapi": "^2.6.2",
		"jest": "^29.7.0",
		"prisma": "^5.20.0",
		"prisma-dbml-generator": "^0.12.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.2",
