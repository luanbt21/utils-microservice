		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"swagger-ui-express": "^5.0.1",
		"ts-proto": "^2.2.4",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
